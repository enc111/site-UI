mixin slider-ruler(min, max, step, widthOfBlock, widthOfNumb)
    -var cols = parseInt((max - min) / step + 1);
    -var iter = cols;
    div.slider__ruler
        while iter--
            div.slider__ruler_number(style= 'margin-right: '+ ((widthOfBlock- widthOfNumb* cols)/ (cols- 1))+ 'rem')= min+ (cols - iter - 1)* step

